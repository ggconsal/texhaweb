}
.section-heading .divider:after {
  content: "";
  position: relative;
  height: 6px;
  width: 80px;
  display: block;
  text-align: center;
  margin: 13px auto;
  -webkit-border-radius: 2px;
  -moz-border-radius: 2px;
  -ms-border-radius: 2px;
  -o-border-radius: 2px;
  border-radius: 2px;
}
.section-heading .divider:after {
  background: #d9534f; /* #f0ad4e; #337AB7; #cc6698; */
}
.section-heading.inverse h1,
.section-heading.inverse p {
  color: #ffffff;
}
.section-heading.inverse .divider:after {
  background: #ffffff;
}

