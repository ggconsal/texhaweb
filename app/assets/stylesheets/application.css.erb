section#about {
	position: relative;
	padding-bottom: 0px;
}

section#about .about-item {
  text-align: center;
  font-size: 17px;
  line-height: 25px;
  color: #999999;
}

section#about .about-item i.fa {
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  -o-border-radius: 50%;
  border-radius: 50%;
  padding: 35px;
  -webkit-transition: all 0.25s ease-in-out;
  -moz-transition: all 0.25s ease-in-out;
  -ms-transition: all 0.25s ease-in-out;
  -o-transition: all 0.25s ease-in-out;
  transition: all 0.25s ease-in-out;
  width: 104px;
}

section#about .about-item i.fa {
  border: 2px solid #434687;
  color: #434687;
}
section#about .about-item:hover i.fa {
  color: #ffffff;
  background: #434687;
}
section#about .about-item:hover h3 {
  color: #434687;
}

#div-blog, #about, #modif-datos, #ingresar {
  background: url('page-background.png');
}

.quienes-image {
	height: auto;
	width: 100%;
	border-radius: 4%;
}

.section-heading {
    margin: 90px 0px 40px;
	text-align: center;
}
.section-heading h1 {
	display: inline-block;
	font-size: 55px;
	margin-bottom: 0;
	font-weight: bold;
	font-style: italic;
}
.section-heading p {
	font-size: 25px;
	color: #999999;
	font-weight: 300;
	line-height: 35px;
}
.section-heading .divider:after {
  content: "";
  position: relative;
  height: 6px;
  width: 80px;
  display: block;
  text-align: center;
  margin: 13px auto;
  -webkit-border-radius: 2px;
  -moz-border-radius: 2px;
  -ms-border-radius: 2px;
  -o-border-radius: 2px;
  border-radius: 2px;
}
section .section-heading .divider:after {
  background: #cc6698;
}
.section-heading.inverse h1,
.section-heading.inverse p {
  color: #ffffff;
}
.section-heading.inverse .divider:after {
  background: #ffffff;
}

.scrollpoint {
  opacity: 0;
}

.scrollpoint.active {
  opacity: 1 \0;
}

.bs-callout {
  padding: 20px;
  padding-bottom: 10px;
  margin-bottom: 20px;
  border: 1px solid #eee;
  border-left-width: 5px;
  border-radius: 3px;
}
.bs-callout h4, .bs-callout h3 {
  margin-top: 0;
  margin-bottom: 5px;
}
.bs-callout p:last-child {
  margin-bottom: 0;
}
.bs-callout code {
  border-radius: 3px;
}

/* Tighten up space between multiple callouts */
.bs-callout + .bs-callout {
  /*margin-top: -5px;*/
}

/* Variations */
.bs-callout-orange {
  border-left-color: #E1AA27;
}
.bs-callout-orange h4, .bs-callout-orange h3 {
  color: #E1AA27;
}
.bs-callout-red {
  border-left-color: #C6352A;
}
.bs-callout-red h4, .bs-callout-red h3 {
  color: #C6352A;
}
.bs-callout-info {
  border-left-color: #1b809e;
}
.bs-callout-info h4, .bs-callout-info h3 {
  color: #1b809e;
}

.bs-callout-green {
  border-left-color: #2C9E1B;
}
.bs-callout-green h4, .bs-callout-green h3 {
  color: #2C9E1B;
}

.quien-div-der {
  background: #FFF;
  position: relative;
  margin: 20px 0;
  border: 7px solid #FFF;
  -webkit-border-radius: 5px;
  -webkit-background-clip: padding-box;
  -moz-border-radius: 5px;
  -moz-background-clip: padding;
  border-radius: 5px;
  background-clip: padding-box;
  -webkit-box-shadow: inset 0 1px #fff,0 0 8px #c8cfe6;
  -moz-box-shadow: inset 0 1px #fff,0 0 8px #c8cfe6;
  box-shadow: inset 0 1px #fff,0 0 8px #c8cfe6;
  color: inset 0 1px #fff,0 0 8px #c8cfe6;
}


